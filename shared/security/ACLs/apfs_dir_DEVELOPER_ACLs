everyone allow readsecurity,limit_inherit
guest deny write,execute,append,writeattr,writeextattr,writesecurity,chown
guest deny add_file,search,add_subdirectory,writeattr,writeextattr,writesecurity,chown,directory_inherit,only_inherit
guest deny list,delete_child,readattr,readextattr
guest deny readsecurity,file_inherit,only_inherit
group:staff allow list,search,readattr,readextattr,readsecurity,directory_inherit,only_inherit
